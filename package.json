    "start": "next start",
    "lint": "next lint",
    "db:create-migration": "dotenv -e .env.local tsx src/database/scripts/create-migration.ts",
    "db:migrate": "dotenv -e .env.local tsx src/database/scripts/migrate.ts",
    "postdb:migrate": "npm run db:generate",
    "db:generate": "dotenv -e .env.local tsx src/database/scripts/generate.ts",
    "db:drop": "dotenv -e .env.local tsx src/database/scripts/drop.ts",
    "billing:create-plans": "dotenv -e .env.local tsx src/lib/billing/create-plans.ts",
    "db:reset": "npm run db:drop && npm run db:migrate && npm run db:generate && npm run billing:create-plans",
    "worker:default": "dotenv -e .env.local tsx -- --watch src/queue/default-worker.ts",
    "queue:schedule-jobs": "dotenv -e .env.local tsx src/queue/schedule-jobs.ts",
    "format": "next lint --fix && prettier --write --ignore-path .gitignore .",
    "typecheck": "tsc --noEmit",
    "queue:dashboard": "dotenv -e .env.local tsx queue-dashboard.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
